{
  "expo": {
    "name": "Communexus",
    "slug": "communexus",
    "owner": "xvanov",
    "version": "1.0.0",
    "sdkVersion": "54.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "assetBundlePatterns": ["**/*"],
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./assets/icon.png",
          "color": "#1E3A8A",
          "sounds": ["./assets/notification.wav"],
          "mode": "production"
        }
      ]
    ],
    "notification": {
      "icon": "./assets/icon.png",
      "color": "#1E3A8A",
      "iosDisplayInForeground": true,
      "androidMode": "default",
      "androidCollapsedTitle": "New message"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.communexus.communexus",
      "infoPlist": {
        "UIBackgroundModes": ["remote-notification", "remote-notification"],
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.communexus.communexus",
      "permissions": ["RECEIVE_BOOT_COMPLETED", "VIBRATE", "WAKE_LOCK"],
      "useNextNotificationsApi": true
    },
    "web": {},
    "extra": {
      "eas": {
        "projectId": "7fe31a8c-79fe-4f75-8856-abc070acc2d1"
      }
    }
  }
}
