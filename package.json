{
  "name": "communexus",
  "version": "1.0.0",
  "description": "AI-powered messaging app for contractors and service business operators",
  "main": "index.js",
  "scripts": {
    "test": "jest --runInBand",
    "test:all": "jest --runInBand --testPathIgnorePatterns='/node_modules/' '/tests/ai/' '/tests/computer-use/'",
    "test:emul": "firebase emulators:exec --only firestore,auth,storage --project demo-communexus \"jest --runInBand\"",
    "test:performance": "npx expo export --platform all && du -sh dist/ && node -e \"const fs=require('fs'); const stats=fs.statSync('dist'); if(stats.size > 50000000) { console.error('Bundle too large!'); process.exit(1); }\"",
    "test:e2e": "wdio run tests/e2e/config/wdio.conf.js",
    "test:e2e:ios": "wdio run tests/e2e/config/wdio.ios.conf.js",
    "test:e2e:ios:simple": "wdio run tests/e2e/config/wdio.ios.conf.js --spec=./tests/e2e/specs/simple-auth.test.js",
    "test:e2e:ios:visual": "ENABLE_VISUAL_CHECKS=true wdio run tests/e2e/config/wdio.ios.conf.js",
    "test:e2e:android": "wdio run tests/e2e/config/wdio.android.conf.js",
    "test:e2e:headless": "wdio run tests/e2e/config/wdio.conf.js --headless",
    "test:ai": "node tests/computer-use/auth-flow.test.js",
    "test:ai:auth": "node tests/computer-use/auth-flow.test.js",
    "appium:doctor": "appium-doctor",
    "appium:server": "appium server",
    "agent:orchestrate": "node scripts/agent/agent-orchestrator.js",
    "agent:env:start": "node scripts/agent/environment-manager.js start",
    "agent:env:stop": "node scripts/agent/environment-manager.js stop",
    "agent:env:status": "node scripts/agent/environment-manager.js status",
    "agent:test": "node scripts/agent/hybrid-test-runner.js",
    "agent:feedback": "node scripts/agent/feedback-collector.js",
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "react-native",
    "expo",
    "firebase",
    "messaging",
    "ai"
  ],
  "author": "Communexus Team",
  "license": "MIT",
  "dependencies": {
    "@expo/vector-icons": "^15.0.2",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-navigation/native": "^7.1.18",
    "@react-navigation/stack": "^7.4.10",
    "expo": "^54.0.18",
    "expo-dev-client": "~6.0.16",
    "expo-notifications": "^0.32.12",
    "expo-status-bar": "^3.0.8",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.5",
    "react-native-gesture-handler": "^2.28.0",
    "react-native-safe-area-context": "^5.6.1",
    "react-native-screens": "~4.16.0",
    "react-native-web": "^0.21.0",
    "twilio": "^4.23.0",
    "typescript": "^5.0.0",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@anthropic-ai/sdk": "^0.67.0",
    "@appium/doctor": "^2.1.15",
    "@eslint/js": "^9.38.0",
    "@firebase/rules-unit-testing": "^5.0.0",
    "@types/jest": "^29.0.0",
    "@types/node": "^24.9.1",
    "@types/react": "~19.1.10",
    "@types/react-native": "~0.73.0",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.46.2",
    "@wdio/cli": "^9.20.0",
    "@wdio/local-runner": "^9.20.0",
    "@wdio/mocha-framework": "^9.20.0",
    "@wdio/spec-reporter": "^9.20.0",
    "appium": "^3.1.0",
    "appium-uiautomator2-driver": "^5.0.6",
    "appium-xcuitest-driver": "^10.2.2",
    "eas-cli": "^16.24.1",
    "eslint": "^9.38.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.0",
    "eslint-plugin-react-native": "^5.0.0",
    "expo-sqlite": "~16.0.8",
    "firebase": "^12.4.0",
    "firebase-admin": "^12.6.0",
    "firebase-tools": "^14.20.0",
    "jest": "^29.0.0",
    "prettier": "^3.6.2",
    "ts-jest": "^29.4.5",
    "webdriverio": "^9.20.0"
  }
}
