{
  "name": "communexus",
  "version": "1.0.0",
  "description": "AI-powered messaging app for contractors and service business operators",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "test": "jest --runInBand",
    "test:emul": "firebase emulators:exec --only firestore,auth,storage --project demo-communexus \"jest --runInBand\"",
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "react-native",
    "expo",
    "firebase",
    "messaging",
    "ai"
  ],
  "author": "Communexus Team",
  "license": "MIT",
  "dependencies": {
    "@expo/vector-icons": "^15.0.2",
    "@types/react": "~19.1.10",
    "expo": "^54.0.18",
    "expo-sqlite": "~16.0.8",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.5",
    "react-native-web": "^0.21.0",
    "typescript": "^5.0.0",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.38.0",
    "@types/jest": "^29.0.0",
    "@types/node": "^24.9.1",
    "@types/react": "^19.1.10",
    "@types/react-native": "~0.73.0",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.46.2",
    "eslint": "^9.38.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.0",
    "eslint-plugin-react-native": "^5.0.0",
    "expo-sqlite": "~13.0.0",
    "firebase": "^10.14.1",
    "firebase-admin": "^12.6.0",
    "firebase-tools": "^14.20.0",
    "jest": "^29.0.0",
    "prettier": "^3.6.2",
    "ts-jest": "^29.4.5"
  }
}
